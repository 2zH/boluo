    "@types/graphql-type-json": "^0.3.2",
    "@types/ioredis": "^4.0.17",
    "@types/jest": "24.0.18",
    "@types/node": "12.7.4",
    "@types/passport-jwt": "^3.0.1",
    "@types/supertest": "2.0.8",
    "@types/uuid": "^3.4.5",
    "husky": "^3.0.5",
