    "@nestjs/serve-static": "^1.0.1",
    "@nestjs/typeorm": "^6.1.3",
    "apollo-server-express": "^2.9.3",
    "argon2": "^0.24.1",
    "boluo-common": "^0.1.4",
    "dotenv": "^8.1.0",
    "express-rate-limit": "^5.0.0",
    "graphql": "^14.5.4",
    "graphql-tag-pluck": "^0.8.4",
    "graphql-type-json": "^0.3.0",
    "hasha": "^5.0.0",
