{
  "presets": [
    "@babel/env",
    "@babel/typescript",
    "@babel/react",
    [
      "@emotion/babel-preset-css-prop",
      {
        "autoLabel": true,
        "labelFormat": "[local]"
      }
    ]
  ],
  "plugins": [
    [
      "babel-plugin-root-import",
      {
        "rootPathPrefix": "@/",
        "rootPathSuffix": "src/"
      }
    ],
    ["emotion", { "sourceMap": true }],
    "@babel/proposal-class-properties",
    ["@babel/plugin-transform-runtime", { "regenerator": true }]
  ]
}
